<div class="row">
  <% cities.each do |city| %>
    <div class="col xs12 x6 m4 l3">
      <div class="card small">
        <div class="card-image">
          <img src="<%= city.image_path %>" >
        </div>
        <div class="card-content">
          <span class="card-title grey-text text-darken-4"><%= city.name %>, <%= city.state %></span>
        </div>
        <div class="card-action">
          <%= link_to "TASKS", city_path(city) %>
        </div>
      </div>
    </div>
  <% end %>
</div>
